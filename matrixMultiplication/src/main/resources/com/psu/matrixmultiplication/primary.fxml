<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.psu.matrixmultiplication.PrimaryController">
   <children>
      <Label fx:id="MainLabel" text="Matrix Multiplication">
          <font>
              <Font name="Times New Roman" size="15.0" />
          </font>
      </Label>
      <Button fx:id="multiplyButton" text="Multiply" onAction="#switchToSecondary"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
